
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option htmode 'HT40-'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'DSSS_CCK-40'
	option hwmode '11ng'
	option disabled '0'
	option noscan '1'
	option channel '6'

config wifi-iface 'ap_0'
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option hidden '0'
	option maxassoc '30'
	option disabled '1'
	option encryption 'none'
	option ssid 'Dragino2-8218fa'

config wifi-iface 'ah_0'
	option device 'radio0'
	option ifname 'wlan0-1'
	option network 'batnet'
	option bssid '02:CA:FF:EE:BA:BE'
	option mode 'adhoc'
	option hidden '0'
	option ssid 'mesh'
	option encryption 'none'
	option disabled '1'

config wifi-iface 'sta_0'
	option device 'radio0'
	option ifname 'wlan0-2'
	option mode 'sta'
	option network 'wan'
	option disabled '1'

