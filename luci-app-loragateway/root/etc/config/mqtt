
config settings 'general'
	option routine_script 'mqtt_process.sh'
	option QoS '1'
	option server_type 'general'
	option port '1883'
	option cafile '/etc/iot/cert/cafile'

config servers 'thingspeak'
	option server 'mqtt.thingspeak.com'
	option fix_server '1'
	option port '1883'
	option topic_format 'channels/CHANNEL/publish/WRITE_API'
	option data_format 'DATA&status=MQTTPUBLISH'

config servers 'mydevices'
	option server 'mqtt.mydevices.com'
	option fix_server '1'
	option port '1883'
	option topic_format 'v1/USERNAME/things/CLIENTID/data/CHANNEL'
	option data_format 'DATA'

config servers 'lewei50'
	option server 'mqtt.lewei50.com'
	option fix_server '1'
	option port '1883'
	option topic_format '/lw/u/CLIENTID/CHANNEL'
	option data_format 'DATA'

config servers 'azureiothub'
	option fix_server '0'
	option port '8883'
	option cafile '/etc/iot/cert/Azure.cer'
	option topic_format 'devices/CLIENTID/messages/events/CHANNEL/'
	option data_format 'DATA'

config servers 'awsiot'
	option fix_server '0'
	option port '8883'
	option cafile '/etc/iot/cert/AmazonRootCA1.pem'
	option topic_format 'CLIENTID/CHANNEL'
	option data_format 'DATA'

